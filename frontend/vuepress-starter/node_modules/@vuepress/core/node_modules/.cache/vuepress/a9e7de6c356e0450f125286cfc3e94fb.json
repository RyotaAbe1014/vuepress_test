urn this._metaMap[name]\n  }\n}\n\nexport default ({ Vue }) => {\n  Vue.mixin({\n    computed: {\n      $tags () {\n        const { pages } = this.$site\n        const tags = new Classifiable(tagMeta, pages)\n        return tags\n      },\n      $tag () {\n        const tagName = this.$route.meta.tagName\n        return this.$tags.getItemByName(tagName)\n      },\n      $categories () {\n        const { pages } = this.$site\n        const categories = new Classifiable(categoryMeta, pages)\n        return categories\n      },\n      $category () {\n        const categoryName = this.$route.meta.categoryName\n        return this.$categories.getItemByName(categoryName)\n      },\n    },\n  })\n}\n"],"mappings":";;;;;;;AAAA;AAEA,SAASA,aAAT,QAA8B,WAA9B;AACA,OAAOC,OAAP,MAAoB,cAApB;AACA,OAAOC,YAAP,MAAyB,mBAAzB;;IAEMC,Y;EACJ,sBAAaC,OAAb,EAAsBC,KAAtB,EAA6B;IAAA;;IAAA;;IAC3B,KAAKC,QAAL,GAAgBC,MAAM,CAACC,MAAP,CAAc,EAAd,EAAkBJ,OAAlB,CAAhB;IACAG,MAAM,CAACE,IAAP,CAAY,KAAKH,QAAjB,EAA2BI,OAA3B,CAAmC,UAAAC,IAAI,EAAI;MACzC,IAAQC,QAAR,GAAqB,KAAI,CAACN,QAAL,CAAcK,IAAd,CAArB,CAAQC,QAAR;MACA,KAAI,CAACN,QAAL,CAAcK,IAAd,EAAoBE,KAApB,GAA4BD,QAAQ,CAACE,GAAT,CAAa,UAAAC,GAAG;QAAA,OAAIf,aAAa,CAACK,KAAD,EAAQU,GAAR,CAAjB;MAAA,CAAhB,CAA5B;IACD,CAHD;EAID;;;;SAED,eAAc;MACZ,OAAOR,MAAM,CAACE,IAAP,CAAY,KAAKH,QAAjB,EAA2BU,MAAlC;IACD;;;SAED,eAAW;MACT,OAAO,KAAKV,QAAZ;IACD;;;SAED,eAAY;MACV,OAAO,KAAKW,OAAL,EAAP;IACD;;;WAED,mBAAW;MAAA;;MACT,IAAMC,IAAI,GAAG,EAAb;MACAX,MAAM,CAACE,IAAP,CAAY,KAAKH,QAAjB,EAA2BI,OAA3B,CAAmC,UAAAC,IAAI,EAAI;QACzC,2BAAwB,MAAI,CAACL,QAAL,CAAcK,IAAd,CAAxB;QAAA,IAAQE,KAAR,wBAAQA,KAAR;QAAA,IAAeM,IAAf,wBAAeA,IAAf;QACAD,IAAI,CAACE,IAAL,CAAU;UAAET,IAAI,EAAJA,IAAF;UAAQE,KAAK,EAALA,KAAR;UAAeM,IAAI,EAAJA;QAAf,CAAV;MACD,CAHD;MAIA,OAAOD,IAAP;IACD;;;WAED,uBAAeP,IAAf,EAAqB;MACnB,OAAO,KAAKL,QAAL,CAAcK,IAAd,CAAP;IACD;;;;;;AAGH,gBAAe,gBAAa;EAAA,IAAVU,GAAU,QAAVA,GAAU;EAC1BA,GAAG,CAACC,KAAJ,CAAU;IACRC,QAAQ,EAAE;MACRC,KADQ,mBACC;QACP,IAAQnB,KAAR,GAAkB,KAAKoB,KAAvB,CAAQpB,KAAR;QACA,IAAMa,IAAI,GAAG,IAAIf,